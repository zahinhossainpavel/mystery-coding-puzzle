#include <iostream>

int main() {
    int n = 5; // You can change this value to adjust the pattern size or you can take a input for pattern size

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            int value = (i + j) % n + 1;
            std::cout << value;
        }
        std::cout << std::endl;
    }

    return 0;
}
